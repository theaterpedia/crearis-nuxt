{
  "name": "@crearis/ui",
  "version": "0.1.0",
  "description": "Vue UI components.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theaterpedia/crearis-nuxt.git",
    "directory": "packages/ui"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./css/*": "./dist/css/*",
    "./fonts/*": "./dist/fonts/*",
    "./nuxt": "./dist/nuxt.ts",
    "./styles": "./dist/css/styles.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --project tsconfig.build.json"
  },
  "dependencies": {
    "@nuxt/kit": "^3.13.2",
    "@vueuse/core": "^10.11.0",
    "focus-trap": "^7",
    "nanoid": "^5.0.7",
    "swiper": "^11.1.8",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@laynezh/vite-plugin-lib-assets": "^0.5.23",
    "@nuxt/schema": "^3.13.2",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.12",
    "@vitejs/plugin-vue": "^5.1.0",
    "@vue/tsconfig": "^0.5.1",
    "nuxt": "^3.13.2",
    "vite": "^5.3.5",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vite-plugin-static-copy": "^1.0.6",
    "vue-tsc": "^2.1.10"
  }
}
