{
  "name": "crearis",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theaterpedia/crearis-nuxt.git"
  },
  "keywords": [
    "ui",
    "components",
    "typescript",
    "vue",
    "nuxt"
  ],
  "type": "module",
  "scripts": {
    "start": "node .output/server/index.mjs ",
    "build": "nuxt build --log-level=verbose",
    "codegen": "graphql-codegen",
    "prepare:sfui": "pnpm --filter=sfui prepare",
    "prepare:main": "nuxt prepare",
    "prepare": "pnpm prepare:main",
    "dev": "pruvious dev --port 3000",
    "dev:theme": "pnpm --filter=theme dev",
    "dev:ui": "pnpm --filter=ui dev",
    "dev:home": "pnpm --filter=home dev",
    "build:theme": "pnpm --filter=theme build",
    "build:ui": "pnpm --filter=ui build",
    "build:home": "pnpm --filter=home build",
    "format": "prettier --write .",
    "postinstall": "pnpm prepare:sfui && pnpm build:ui && pnpm prepare && pnpm run codegen",
    "test": "vitest",
    "testui": "vitest --ui"
  },
  "dependencies": {
    "@apollo/client": "^3.8.6",
    "@azure/msal-node": "^2.7.0",
    "@crearis/odoo-sdk": "^0.7.9",
    "@crearis/odoo-sdk-api-client": "^0.7.9",
    "@crearis/sfui": "workspace:*",
    "@crearis/tailwind-config": "workspace:*",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.0",
    "@nuxt/kit": "3.13.2",
    "@nuxt/image": "1.7.0",
    "@vue-storefront/sdk": "^2.0.0",
    "@vueuse/core": "^11.0.0",
    "currency.js": "^2.0.4",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "nuxt": "3.13.2",
    "nitropack": "2.9.7",
    "c12": "1.11.2",
    "object-hash": "^3.0.0",
    "nuxt-lazy-hydrate": "^1.0.0",
    "nuxt-lodash": "^2.5.3",
    "prettier": "^3.3.0",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "pruvious": "^3.13.2",
    "react": "^18.2.0",
    "vue": "^3.5.8",
    "vue-router": "^4.4.5",
    "vue-slider-component": "4.1.0-beta.7",
    "vue-toastification": "next",
    "@floating-ui/vue": "^1.1.0",
    "@storefront-ui/shared": "^2.5.0",
    "jw-paginate": "^1.0.4",
    "tabbable": "^6.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/i18n": "^8.3.1",
    "@nuxtjs/tailwindcss": "6.12.1",
    "@vitest/ui": "^2.1.2",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tw-colors": "^3.3.1",
    "vite": "^5.3.5",
    "vitest": "^2.1.2"
  },
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=20.0.0"
  }
}
